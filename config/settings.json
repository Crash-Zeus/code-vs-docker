{
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontWeight": "400",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.mouseWheelZoom": true,
    "window.titleBarStyle": "custom",
    "editor.minimap.scale": 1,
    "window.zoomLevel": -1,
    "editor.fontSize": 14,
    "files.encoding": "utf8",
    "[python]": {
        "files.encoding": "utf8"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "files.encoding": "utf8"
    },
    "[php]": {
        "files.encoding": "utf8"
    },
    "git.confirmSync": false,
    "editor.multiCursorModifier": "alt",
    "editor.formatOnPaste": false,
    "php.executablePath": "/usr/bin/php",
    "php.validate.enable": true,
    "php.validate.executablePath": "/usr/bin/php",
    "php.validate.run": "onSave",
    "editor.cursorWidth": 3,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "consolas",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.letterSpacing": 1,
    "debug.allowBreakpointsEverywhere": true,
    "debug.toolBarLocation": "docked",
    "workbench.activityBar.visible": true,
    "editor.renderWhitespace": "all",
    "git.enableSmartCommit": true,
    "git.autofetch": false,
    "workbench.preferredDarkColorTheme": "Monokai",
    "editor.minimap.enabled": false,
    "discord.workspaceElapsedTime": true,
    "phpcs.enable": true,
    "phpcs.executablePath": "/usr/bin/phpcs",
    "launch": {
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.type.class",
                    "keyword.operator.expression",
                    "storage.modifier",
                    "storage.type.class",
                    "keyword.other.use",
                    "keyword.control.return",
                    "storage.type.function.python",
                    "support.function.js",
                    "support.function.console.js",
                    "storage.type.js",
                    "keyword.control.flow.js",
                    "keyword.other.namespace.php",
                    "storage.type.function.php",
                    "support.function.magic.php"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "keyword.operator.new.js"
                ],
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "variable.other.property.js"
                ],
                "settings": {
                    "foreground": "#80bfff"
                }
            }
        ]
    },
    "phpcs.warningSeverity": 5,
    "phpcs.errorSeverity": 0,
    "breadcrumbs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "python.showStartPage": false,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.pythonPath": "/usr/bin/python3",
    "scm.alwaysShowRepositories": true,
    "intelephense.environment.phpVersion": "8.0",
    "terminal.integrated.shell.linux" : "/bin/bash",
    "workbench.startupEditor":"none",
    "update.mode": "none"
}